{
  "version": "0.1.0",
  "name": "villager-bingo",
  "description": "Bingo card generator/game for villager hunting in Animal Crossing: New Horizons ",
  "scripts": {
    "build": "parcel build src/index.html --out-dir ./ --public-url ./",
    "dev": "parcel src/index.html -p 9000",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "eslint": "^6.0.1",
    "eslint-config-personal": "github:verygooddev/eslint-config-personal",
    "eslint-config-preact": "^1.1.0",
    "parcel-bundler": "^1.12.4",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3",
    "stylus": "^0.54.8"
  },
  "resolutions": {
    "terser": "^5.2.0"
  },
  "eslintConfig": {
    "extends": [
      "preact",
      "personal"
    ],
    "ignorePatterns": [
      "build/"
    ],
    "settings": {
      "react": {
        "pragma": "h"
      }
    },
    "rules": {
      "consistent-return": "off",
      "default-case": "off",
      "react/prop-types": "off",
      "react/jsx-props-no-spreading": "off"
    }
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 and_chr versions",
    "last 2 edge versions",
    "last 2 firefox versions",
    "last 2 and_ff versions",
    "last 2 ios versions",
    "last 2 safari versions"
  ],
  "author": {
    "email": "very_good_dev@outlook.com",
    "name": "VeryGoodDev"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "url": "https://github.com/verygooddev/villager-bingo"
  }
}
