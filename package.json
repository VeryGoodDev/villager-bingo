{
  "version": "0.1.0",
  "name": "villager-bingo",
  "description": "Bingo card generator/game for villager hunting in Animal Crossing: New Horizons ",
  "scripts": {
    "build": "parcel build src/index.html --out-dir ./ --public-url ./",
    "dev": "preact watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "serve": "sirv build --port 8080 --cors --single",
    "start": "parcel index.html -p 9000"
  },
  "dependencies": {
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "eslint": "^6.0.1",
    "eslint-config-personal": "github:verygooddev/eslint-config-personal",
    "eslint-config-preact": "^1.1.0",
    "parcel-bundler": "^1.12.4",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3"
  },
  "eslintConfig": {
    "extends": [
      "preact",
      "personal"
    ],
    "ignorePatterns": [
      "build/"
    ],
    "settings": {
      "react": {
        "pragma": "h"
      }
    }
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "author": {
    "email": "very_good_dev@outlook.com",
    "name": "VeryGoodDev"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "url": "https://github.com/verygooddev/villager-bingo"
  }
}
